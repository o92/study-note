{
  "name": "study-note",
  "displayName": "study note",
  "publisher": "hyj",
  "description": "For some extensions in vscode notebooks,it is mainly to turn a notebook into a study note, please refer to readme for details.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "keywords": [
    "notebook",
    "notebooks",
    "markdown",
    "katex",
    "Markdown"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/o92/vscode-notebook-extension"
  },
  "bugs": {
    "url": "https://github.com/o92/vscode-notebook-extension/issues"
  },
  "categories": [
    "Notebooks",
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./out/extension.js",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "bun run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "bun run compile && bun run lint",
    "lint": "eslint src --ext ts",
    "test": "bun ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.83.0",
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "@vscode/test-electron": "^2.3.4"
  },
  "dependencies": {
    "@vscode/vsce": "^2.21.1"
  }
}